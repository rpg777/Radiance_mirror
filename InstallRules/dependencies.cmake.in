fixup_bundle("${CMAKE_INSTALL_PREFIX}/bin/rtrace@CMAKE_EXECUTABLE_SUFFIX@" "${plugins}" "@QT_LIBRARY_DIR@;@X11_LIBRARY_DIR@;@TIFF_LIBRARY_DIRS@")
